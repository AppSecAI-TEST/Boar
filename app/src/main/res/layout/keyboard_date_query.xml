<?xml version="1.0" encoding="utf-8"?>
<com.zhy.android.percent.support.PercentLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/percent"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    android:orientation="vertical">

    <com.zhy.android.percent.support.PercentLinearLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_gravity="center"
        android:background="@drawable/background_white_4"
        android:orientation="vertical"
        app:layout_heightPercent="88%h"
        app:layout_marginTopPercent="5%w"
        app:layout_widthPercent="60%w">

        <com.zhy.android.percent.support.PercentLinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_marginLeftPercent="6%w"
                app:layout_marginTopPercent="2%w">
                <!--1.1-->
                <com.zhy.android.percent.support.PercentLinearLayout
                    android:id="@+id/input_ll_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@mipmap/keyboard_2"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="开始日期:"
                        android:textColor="#078d82"
                        app:layout_marginLeftPercent="1%w"
                        app:layout_textSizePercent="4%w" />

                    <EditText
                        android:id="@+id/input_1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:background="@null"
                        android:cursorVisible="false"
                        android:hint="例20170603"
                        android:textColorHint="#A1A1A1"
                        android:textCursorDrawable="@null"
                        app:layout_marginLeftPercent="1%w"
                        app:layout_textSizePercent="4%w" />
                    <!--1.1-->
                </com.zhy.android.percent.support.PercentLinearLayout>

                <com.zhy.android.percent.support.PercentLinearLayout
                    android:id="@+id/input_ll_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@mipmap/keyboard_2"
                    android:orientation="horizontal"
                    app:layout_marginTopPercent="2%w">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="结束日期:"
                        android:textColor="#078d82"
                        app:layout_marginLeftPercent="1%w"
                        app:layout_textSizePercent="4%w" />

                    <EditText
                        android:id="@+id/input_2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:background="@null"
                        android:hint="例20170603"
                        android:textColorHint="#A1A1A1"
                        android:textCursorDrawable="@null"
                        app:layout_marginLeftPercent="1%w"
                        app:layout_textSizePercent="4%w" />
                    <!--1.1-->
                </com.zhy.android.percent.support.PercentLinearLayout>
                <!--1-->
            </com.zhy.android.percent.support.PercentLinearLayout>

            <Button
                android:id="@+id/bu_w"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@mipmap/keyboard_3"
                android:text="完成"
                android:textColor="#fff"
                app:layout_marginLeftPercent="12%w"
                app:layout_textSizePercent="4%w" />

        </com.zhy.android.percent.support.PercentLinearLayout>
        <!--/第二层-->
        <com.zhy.android.percent.support.PercentLinearLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_gravity="center_horizontal"
            android:orientation="vertical"
            app:layout_heightPercent="65%h"
            app:layout_marginTopPercent="1%w"
            app:layout_widthPercent="82%w">

            <!--2-->
            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:gravity="center"
                android:orientation="horizontal"
                app:layout_heightPercent="22%h"
                app:layout_marginTopPercent="1%w"
                app:layout_widthPercent="100%w">

                <Button
                    android:id="@+id/bu_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@mipmap/keyboard_1"
                    android:enabled="false"
                    android:text="1"
                    app:layout_textSizePercent="7%w" />

                <Button
                    android:id="@+id/bu_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:background="@mipmap/keyboard_1"
                    android:enabled="false"
                    android:text="2"
                    app:layout_textSizePercent="7%w" />

                <Button
                    android:id="@+id/bu_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:background="@mipmap/keyboard_1"
                    android:enabled="false"
                    android:text="3"
                    app:layout_textSizePercent="7%w" />
                <!--2-->
            </com.zhy.android.percent.support.PercentLinearLayout>

            <!--3-->
            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:gravity="center"
                android:orientation="horizontal"
                app:layout_heightPercent="22%h"
                app:layout_marginTopPercent="1%w"
                app:layout_widthPercent="100%w">

                <Button
                    android:id="@+id/bu_4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@mipmap/keyboard_1"
                    android:enabled="false"
                    android:text="4"
                    app:layout_textSizePercent="7%w" />

                <Button
                    android:id="@+id/bu_5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:background="@mipmap/keyboard_1"
                    android:enabled="false"
                    android:text="5"
                    android:textSize="35dp" />

                <Button
                    android:id="@+id/bu_6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:background="@mipmap/keyboard_1"
                    android:enabled="false"
                    android:text="6"
                    android:textSize="35dp" />
                <!--3-->
            </com.zhy.android.percent.support.PercentLinearLayout>

            <!--4-->
            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:gravity="center"
                android:orientation="horizontal"
                app:layout_heightPercent="22%h"
                app:layout_marginTopPercent="1%w"
                app:layout_widthPercent="100%w">

                <Button
                    android:id="@+id/bu_7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@mipmap/keyboard_1"
                    android:enabled="false"
                    android:text="7"
                    android:textSize="35dp" />

                <Button
                    android:id="@+id/bu_8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:background="@mipmap/keyboard_1"
                    android:enabled="false"
                    android:text="8"
                    android:textSize="35dp" />

                <Button
                    android:id="@+id/bu_9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:background="@mipmap/keyboard_1"
                    android:enabled="false"
                    android:text="9"
                    android:textSize="35dp" />
                <!--4-->
            </com.zhy.android.percent.support.PercentLinearLayout>

            <!--5-->
            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:gravity="center"
                android:orientation="horizontal"
                app:layout_heightPercent="22%h"
                app:layout_marginTopPercent="1%w"
                app:layout_widthPercent="100%w">

                <Button
                    android:id="@+id/delete"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@mipmap/keyboard_1"
                    android:enabled="false"
                    android:text="删除"
                    android:textSize="30dp" />

                <Button
                    android:id="@+id/bu_0"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:background="@mipmap/keyboard_1"
                    android:enabled="false"
                    android:text="0"
                    android:textSize="35dp" />

                <Button
                    android:id="@+id/out"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:background="@mipmap/keyboard_1"
                    android:text="退出"
                    android:textSize="30dp" />
                <!--5-->
            </com.zhy.android.percent.support.PercentLinearLayout>

            <!--第二层-->
        </com.zhy.android.percent.support.PercentLinearLayout>


    </com.zhy.android.percent.support.PercentLinearLayout>
</com.zhy.android.percent.support.PercentLinearLayout>