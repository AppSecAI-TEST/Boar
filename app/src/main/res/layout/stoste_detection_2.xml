                            android:text="打印"
                            android:textColor="#f4f4f4"
                            app:layout_heightPercent="40%h"
                            app:layout_marginLeftPercent="4%w"
                            app:layout_textSizePercent="6%w"
                            app:layout_widthPercent="40%w" />

                        <Button
                            android:id="@+id/bu_return"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_gravity="center_vertical"
                            android:background="@drawable/background_green"
                            android:text="返回"
                            android:textColor="#f4f4f4"
                            app:layout_heightPercent="40%h"
                            app:layout_marginLeftPercent="8%w"
                            app:layout_textSizePercent="6%w"
                            app:layout_widthPercent="40%w" />

                    </com.zhy.android.percent.support.PercentLinearLayout>

                    <com.zhy.android.percent.support.PercentLinearLayout
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_gravity="center_vertical"
                        android:background="@drawable/background_white_1"
                        android:orientation="horizontal"
                        app:layout_heightPercent="60%h"
                        app:layout_widthPercent="100%w">

                        <!--2.3.1-->
                        <com.zhy.android.percent.support.PercentLinearLayout
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_gravity="center_vertical"
                            android:background="#ecf5f4"
                            android:orientation="vertical"
                            app:layout_heightPercent="100%h"
                            app:layout_widthPercent="50%w">


                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:layout_marginLeft="22dp"
                                android:gravity="center_vertical"
                                android:text="密   度"
