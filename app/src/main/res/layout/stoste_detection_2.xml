                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_marginLeft="22dp"
                            android:gravity="center_vertical"
                            android:text="800毫升"
                            app:layout_heightPercent="14%h"
                            app:layout_textSizePercent="10%w"
                            app:layout_widthPercent="90%w" />

                    </com.zhy.android.percent.support.PercentLinearLayout>
                    <!--2.2.1-->
                </com.zhy.android.percent.support.PercentLinearLayout>
                <!--2.2.2-->
                <com.zhy.android.percent.support.PercentLinearLayout
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:orientation="vertical"
                    app:layout_heightPercent="90%h"
                    app:layout_marginLeftPercent="8%w"
                    app:layout_widthPercent="40%w">

                    <com.zhy.android.percent.support.PercentLinearLayout
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        app:layout_heightPercent="34%h"
                        app:layout_widthPercent="100%w">

                        <Button
                            android:id="@+id/print"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_gravity="center_vertical"
