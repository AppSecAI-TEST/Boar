<?xml version="1.0" encoding="utf-8"?>
<com.zhy.android.percent.support.PercentLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.zhy.android.percent.support.PercentLinearLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_heightPercent="20%h"
        app:layout_widthPercent="100%w">

        <TextView
            android:id="@+id/date"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:text="日期"
            android:textColor="#089285"
            android:textSize="30dp"
            app:layout_heightPercent="100%h"
            app:layout_widthPercent="15%w" />

        <View
            android:layout_width="1dp"
            android:layout_height="83dp"
            android:background="#000" />

        <TextView
            android:id="@+id/time"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center"
            android:text="时间"
            android:textColor="#089285"
            android:textSize="30dp"
            app:layout_heightPercent="100%h"
            app:layout_widthPercent="11%w" />

        <View
            android:layout_width="1dp"
            android:layout_height="83dp"
            android:background="#000" />

        <TextView
            android:id="@+id/type"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center"
            android:text="类型"
            android:textColor="#089285"
            android:textSize="30dp"
            app:layout_heightPercent="100%h"
            app:layout_widthPercent="12%w" />

        <View
            android:layout_width="1dp"
            android:layout_height="83dp"
            android:background="#000" />


        <TextView
            android:id="@+id/density"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center"
            android:text="密度"
            android:textColor="#089285"
            android:textSize="30dp"
            app:layout_heightPercent="100%h"
            app:layout_widthPercent="12%w" />


        <View
            android:layout_width="1dp"
            android:layout_height="83dp"
            android:background="#000" />

        <TextView
            android:id="@+id/vitality"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center"
            android:text="活力"
            android:textColor="#089285"
            android:textSize="30dp"
            app:layout_heightPercent="100%h"
            app:layout_widthPercent="10%w" />

        <View
            android:layout_width="1dp"
            android:layout_height="83dp"
            android:background="#000" />

        <TextView
            android:id="@+id/motilityRate"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center"
            android:text="活率"
            android:textColor="#089285"
            android:textSize="30dp"
            app:layout_heightPercent="100%h"
            app:layout_widthPercent="10%w" />

        <View
            android:layout_width="1dp"
            android:layout_height="83dp"
            android:background="#000" />

        <TextView
            android:id="@+id/operator"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center"
            android:text="操作员"
            android:textColor="#089285"
            android:textSize="30dp"
            app:layout_heightPercent="100%h"
            app:layout_widthPercent="10%w" />

        <View
            android:layout_width="1dp"
            android:layout_height="83dp"
            android:background="#000" />

        <TextView
            android:id="@+id/result"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:text="结果"
            android:textColor="#089285"
            android:textSize="30dp"
            app:layout_heightPercent="100%h"
            app:layout_widthPercent="10%w" />

        <View
            android:layout_width="1dp"
            android:layout_height="83dp"
            android:background="#000" />

        <TextView
            android:id="@+id/check"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center"
            android:paddingLeft="10dp"
            android:text="查看"
            android:textColor="#089285"
            android:textSize="30dp"
            app:layout_heightPercent="100%h"
            app:layout_widthPercent="10%w" />
    </com.zhy.android.percent.support.PercentLinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#000" />
</com.zhy.android.percent.support.PercentLinearLayout>